module ColorRecord exposing (..)


type alias ColorRecord =
    { red : Int
    , green : Int
    , blue : Int
    }


type alias Rgba255Record =
    { red : Int
    , green : Int
    , blue : Int
    , alpha : Int
    }


type alias RgbaFloatRecord =
    { red : Float
    , green : Float
    , blue : Float
    , alpha : Float
    }
